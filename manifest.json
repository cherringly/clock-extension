{
    "manifest_version": 3,
    "name": "Website Time Tracker",
    "version": "1.0",
    "description": "Tracks time spent on websites.",
    "permissions": ["activeTab", "tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "clock.png",
        "48": "clock.png",
        "128": "clock.png"
      }
    },
    "icons": {
      "16": "clock.png",
      "48": "clock.png",
      "128": "clock.png"
    }
  }
  